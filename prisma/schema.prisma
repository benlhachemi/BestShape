// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Program {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  slug     String @unique
  overview Json
  workout  Json
  diet     Json
}
